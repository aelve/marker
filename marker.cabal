name:                marker
version:             0.1.0.0
synopsis:            Mark places in code that depend on other code
description:
  Mark places in code that depend on other code
homepage:            http://github.com/aelve/marker
bug-reports:         http://github.com/aelve/marker/issues
license:             BSD3
license-file:        LICENSE
author:              Artyom
maintainer:          yom@artyom.me
-- copyright:           
category:            Development
tested-with:         GHC == 7.8.4, GHC == 7.10.3, GHC == 8.0.1, GHC == 8.0.2
build-type:          Simple
extra-source-files:  CHANGELOG.md
cabal-version:       >=1.10

source-repository head
  type:                git
  location:            git://github.com/aelve/marker.git

library
  exposed-modules:     Marker
  -- other-modules:       
  -- other-extensions:    
  build-depends:       base >=4.7 && <5
  ghc-options:         -Wall -fno-warn-unused-do-bind
  hs-source-dirs:      lib
  default-language:    Haskell2010
